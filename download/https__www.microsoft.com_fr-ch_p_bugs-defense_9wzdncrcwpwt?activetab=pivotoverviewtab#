        <button class="c-flipper f-previous x-hidden-focus" aria-label="previous" aria-hidden="true" tabindex="-1"></button>

                <header class="m-heading-4" role="tablist">
                        <a id="pivot-tab-OverviewTab" href="#activetab=pivot:overviewtab" rel="nofollow" role="tab" aria-controls="pivot-OverviewTab" class="f-active c-pivot-overviewtab"  data-m='{"cN":"tab-OverviewTab","id":"nn1m1r3a2","sN":1,"aN":"m1r3a2"}' data-tv-entry-rect-left="40" data-tv-entry-rect-right="10000" data-tv-exit-rect-left="40" data-tv-exit-rect-right="40" data-override-focus-sound="true">Vue d’ensemble</a>
                        <a id="pivot-tab-regionOfSystemRequirementsTab" href="#activetab=pivot:regionofsystemrequirementstab" rel="nofollow" role="tab" aria-controls="pivot-regionOfSystemRequirementsTab" class=" c-pivot-regionofsystemrequirementstab"  data-m='{"cN":"tab-regionOfSystemRequirementsTab","id":"nn2m1r3a2","sN":2,"aN":"m1r3a2"}' data-tv-entry-rect-left="40" data-tv-entry-rect-right="10000" data-tv-exit-rect-left="40" data-tv-exit-rect-right="40" data-override-focus-sound="true">Configuration requise</a>
                        <a id="pivot-tab-ReviewsTab" href="#activetab=pivot:reviewstab" rel="nofollow" role="tab" aria-controls="pivot-ReviewsTab" class=" c-pivot-reviewstab"  data-m='{"cN":"tab-ReviewsTab","id":"nn3m1r3a2","sN":3,"aN":"m1r3a2"}' data-tv-entry-rect-left="40" data-tv-entry-rect-right="10000" data-tv-exit-rect-left="40" data-tv-exit-rect-right="40" data-override-focus-sound="true">Avis</a>
