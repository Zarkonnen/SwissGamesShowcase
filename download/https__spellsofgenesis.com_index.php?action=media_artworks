    <script type="text/javascript">
        function tumblr(resp) {
            var posts = resp.posts;
            $('#blog .loading').replaceWith('<div/>');
            var $ul = $('#blog div');
            for (var i=0; i<posts.length; i++) {
                var p = posts[i];
                var title = p['regular-title'] || p['link-text'] || null;
                var body = p['regular-body'];
                if (title)
                    $ul.append('<div class="text-center"><div class="title title--bold"><h3><p><strong>'+title+'</strong></p></h3></div></div>');
                $ul.append('<div class="text" style="margin-top:-80px;"><p>'+body+'</p></div><p>&nbsp;</p><div class="decor"></div>')
            }
        }
    </script>
